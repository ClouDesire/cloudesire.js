{
  "name": "cloudesire.js",
  "version": "1.0.0",
  "description": "ClouDesire API Client",
  "main": "lib/client.js",
  "watch": {
    "test": "{src,test}/*.js"
  },
  "scripts": {
    "compile": "babel src -d lib && browserify src/client.js -t [babelify --no-sourceMaps] -p [minifyify --no-map] -s cd -o lib/client.web.js",
    "prepublish": "npm run compile",
    "docs": "documentation build src/client.js -o README.md -f md",
    "test": "npm run eslint && npm run compile && mocha --compilers js:babel-register",
    "eslint": "eslint src",
    "dev": "npm run watch",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClouDesire/cloudesire.js.git"
  },
  "keywords": [
    "ClouDesire",
    "js",
    "es6",
    "client",
    "rest"
  ],
  "author": "dev@cloudesire.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ClouDesire/cloudesire.js/issues"
  },
  "homepage": "https://github.com/ClouDesire/cloudesire.js#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-plugin-transform-undefined-to-void": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "eslint": "^2.4.0",
    "fetch-mock": "^4.1.1",
    "minifyify": "^7.3.2",
    "mocha": "^2.4.5",
    "mockery": "^1.4.1"
  },
  "dependencies": {
    "fetchival": "^0.3.1",
    "node-fetch": "^1.3.3"
  }
}
